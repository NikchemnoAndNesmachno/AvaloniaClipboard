<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:AvaloniaClipboard.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaClipboard.Views.UserControls.ThemeList"
             x:DataType="viewModels:SettingsViewModel">
    <Design.DataContext>
        <viewModels:SettingsViewModel/>
    </Design.DataContext>
    <ComboBox
        SelectedIndex="{Binding SelectedColorIndex, Mode=TwoWay}">
        <ComboBoxItem Content="{DynamicResource ThemeDark}"/>
        <ComboBoxItem Content="{DynamicResource ThemePink}"/>
    </ComboBox>
</UserControl>
