<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style Selector="ToolTip">
        <Setter Property="Background" Value="{DynamicResource OwnTipBackground}"/>
        <Setter Property="Foreground" Value="{DynamicResource OwnTipForeground}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource OwnTipForeground}"/>
        <Setter Property="CornerRadius" Value="0"/>
    </Style>
</Styles>
